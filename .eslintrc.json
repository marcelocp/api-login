{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "google"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "quotes": [
            2,
            "double"
        ],
        "semi": [
            2,
            "always"
        ],
        "object-curly-spacing": [
            2,
            "always"
        ],
        "require-jsdoc": "off"
    }
}